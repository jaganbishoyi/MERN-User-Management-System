<% for (let index=0; index < users.length; index++) { %>
    <tr>
        <td>
            <%= index+1 %>
        </td>
        <td>
            <%= users[index].name %>
        </td>
        <td>
            <%= users[index].email %>
        </td>
        <td>
            <%= users[index].gender %>
        </td>
        <td>
            <%= users[index].status %>
        </td>
        <td>
            <a href="/update-user?id=<%= users[index]._id%>" class="btn border-shadow update">
                <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
            </a>
            <a class="btn border-shadow delete" data-id="<%= users[index]._id%>">
                <span class="text-gradient"><i class="fas fa-times"></i></span>
            </a>
        </td>
    </tr>
    <% } %>
